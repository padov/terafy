# Configurações do Banco de Dados
DB_HOST=localhost
DB_PORT=5432
DB_NAME=terafy_db
DB_USER=postgres
DB_PASSWORD=mysecretpassword

# SSL do Banco (disable para local, require para produção)
DB_SSL_MODE=disable

# Configurações do Servidor
SERVER_PORT=8080

# Configurações JWT
JWT_SECRET_KEY=sua-chave-secreta-super-segura-aqui-mude-em-producao
JWT_EXPIRATION_DAYS=7

# Configurações do Nginx (opcional)
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

